    "@fortawesome/react-fontawesome": "^0.1.7",
    "axios": "^0.19.2",
    "draft-js": "^0.10.5",
    "draftjs-to-html": "^0.8.4",
    "html-to-draftjs": "^1.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-draft-wysiwyg": "^1.14.4",
    "react-modal": "^3.11.1",
    "react-router-dom": "^5.0.0",
    "react-table": "^6.11.5",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@types/draft-js": "^0.10.38",
    "@types/jest": "24.0.13",
    "@types/node": "12.0.2",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
